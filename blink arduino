int led0 = 8;
int led1 = 9;
int led2 = 10;
int led3 = 11;

void setup()
{
pinMode(8, OUTPUT);
pinMode(9, OUTPUT);
pinMode(10, OUTPUT);
pinMode(11, OUTPUT);
}

void loop()
{
digitalWrite(led0, HIGH);
delay(666);
digitalWrite(led0. LOW);
delay(666);
{
digitalWrite(led1, HIGH);
delay(666);
digitalWrite(led1, LOW);
delay(666);
}
{
digitalWrite(led2, HIGH);
delay(666);
digitalWrite(led2, LOW);
delay(666)
}
{
digitalWrite(led3, HIGH);
delay(666);
digitalWrite(led3, HIGH);
delay(666);
}
}
